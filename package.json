{
  "name": "finex.io",
  "version": "1.1.0",
  "private": true,
  "description": "finex.io",
  "author": "Maksim Amelchenko <m.amelchenko@gmail.com>",
  "keywords": [
    "Budget Tracker & Planner",
    "Online Money Management",
    "FINEX.io"
  ],
  "scripts": {
    "develop": "gatsby develop --host=0.0.0.0",
    "start": "gatsby develop",
    "build": "gatsby build && [ $GIT_TAG ] && version=$GIT_TAG:$GIT_HASH || version=$npm_package_version:$GIT_HASH && printf $version > ./public/version",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "generate-translations": "ts-node -P i18n/tsconfig.json i18n/scanForI18N.ts && prettier --write './locales/*.js'",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prepare": "husky install"
  },
  "dependencies": {
    "@svgr/webpack": "^6.5.1",
    "clsx": "^1.2.1",
    "date-fns": "^2.29.3",
    "deepmerge": "^4.2.2",
    "gatsby": "^5.2.0",
    "gatsby-plugin-google-gtag": "^5.2.0",
    "gatsby-plugin-image": "^3.2.0",
    "gatsby-plugin-manifest": "^5.2.0",
    "gatsby-plugin-react-helmet": "^6.2.0",
    "gatsby-plugin-sass": "^6.2.0",
    "gatsby-plugin-sharp": "^5.2.0",
    "gatsby-plugin-sitemap": "^6.2.0",
    "gatsby-plugin-svgr": "^3.0.0-beta.0",
    "gatsby-source-filesystem": "^5.2.0",
    "gatsby-theme-i18n": "^3.0.0",
    "gatsby-transformer-sharp": "^5.2.0",
    "i18n-js": "^3.9.2",
    "lodash.get": "^4.4.2",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "sass": "^1.56.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@fontsource/nunito": "^4.5.11",
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/builder-webpack5": "^6.5.13",
    "@storybook/manager-webpack5": "^6.5.13",
    "@storybook/react": "^6.5.13",
    "@types/react": "^18.0.25",
    "babel-loader": "^9.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^7.0.0",
    "postcss": "^8.4.19",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  }
}
